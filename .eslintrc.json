{
  "env": {
    "browser": true,
    "node": true,
    "es2022": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["cypress", "jest-dom", "jest", "solid", "testing-library"],
  "extends": [
    "eslint:recommended",
    "plugin:cypress/recommended",
    "plugin:jest-dom/recommended",
    "plugin:jest/recommended",
    "plugin:solid/recommended"
  ]
}
